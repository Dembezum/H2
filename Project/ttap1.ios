hostname ttap1
!

interface BVI1
 ip address 192.168.0.2 255.255.255.0
 ip helper-address 172.16.0.22
 no shutdown
!
dot11 ssid tts
 authentication open
 authentication key-management wpa version 2
 guest-mode
 wpa-psk ascii kaffe12345
!
interface dot11Radio1
 encryption mode ciphers aes-ccm
 ssid tts
 no shutdown
!
ip domain-name tts.local
crypto key generate rsa general-keys modulus 1024
ip ssh version 2
username ttap1 secret cisco
enable secret cisco
!
line con 0
login local
password cisco
!
line vty 0 15
login local
password cisco
transport input ssh
